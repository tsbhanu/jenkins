pipeline{
agent {
label 'maven_build_server'
}
 tools {
        maven 'Maven_3.8.3'
    }
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
sh "mvn clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
sh "mvn test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
sh "mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}

