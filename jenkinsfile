pipeline{
agent maven_build_server
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
mvn clean
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
mvn test
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
mvn compile
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
